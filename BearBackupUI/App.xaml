<Application x:Class="BearBackupUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BearBackupUI"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             Startup="OnStartup" Exit="OnExit"
             ShutdownMode="OnLastWindowClose" SessionEnding="Application_SessionEnding"
             DispatcherUnhandledException="OnDispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--<ui:ThemesDictionary Theme="Light" />-->
                <ui:ControlsDictionary />
                <ResourceDictionary Source="Resources/ImageDictionary.xaml" />
                <ResourceDictionary Source="Resources/ControlDictionary.xaml" />
                <ResourceDictionary Source="Resources/HelperDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
